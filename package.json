{
  "name": "meditechhire-website",
  "version": "1.0.0",
  "description": "Modern, responsive website for MediTech Hire - Medical & Technical Recruitment",
  "main": "index.html",
  "scripts": {
    "start": "live-server --port=3000 --open=/",
    "build": "npm run minify-css && npm run minify-js",
    "minify-css": "cleancss -o styles.min.css styles.css",
    "minify-js": "terser script.js -o script.min.js --compress --mangle",
    "serve": "http-server -p 3000 -o",
    "lint": "eslint script.js",
    "optimize": "npm run build && npm run compress-images"
  },
  "devDependencies": {
    "live-server": "^1.2.2",
    "http-server": "^14.1.1",
    "clean-css-cli": "^5.6.2",
    "terser": "^5.19.4",
    "eslint": "^8.48.0"
  },
  "keywords": [
    "medical recruitment",
    "technical recruitment",
    "healthcare jobs",
    "technology careers",
    "vendor partner",
    "responsive website"
  ],
  "author": "MediTech Hire",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}